<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kosa.metacar.mapper.CarMapper">

	<select id="cargetList" resultType="kosa.metacar.dto.Have_CarDTO">
		<![CDATA[
			select * from have_car
		]]>
	</select>
	
	
	
	<select id="cargetListWithPaging" resultType="kosa.metacar.dto.Have_CarDTO">
   	<![CDATA[
			select carNum, carModel, carMaking from
            (select /*+INDEX_DESC(tbl_board pk_board) */
                    rownum rn, carNum, carModel, carMaking
                    from have_car 
                    where rownum <= 20)
        	where rn > 10
	]]>
	</select>
</mapper>